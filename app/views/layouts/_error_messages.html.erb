<% if object.errors.any? %>
  <div id="error_explanation">
   <div class="alert alert-error">
     The form contains <%= pluralize(object.errors.count, "error") %>.
   </div>
  <ul>
<%# logger.info(object.errors.to_yaml) %>
<%# logger.info(object.errors.values.to_yaml) %>
   <% object.errors.values.each do |msg| %>
<% logger.info(msg.to_yaml) %>
    <li><%= msg.first %></li>
   <% end %>
 </ul>
</div>
<% end %>
